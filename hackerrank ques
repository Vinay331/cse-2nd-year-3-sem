#include <bits/stdc++.h>
using namespace std;
int main()
{
    int x,y,z,p,q,r,i,j,m,n;
    int a[3];
    int b[3];
    cout<<"enter a";
    for(i=0;i<3;i++)
    {
        cin>>a[i];
    }
    cout<<"enter b";
    for(j=0;j<3;j++)
    {
        cin>>b[j];
    }
    int c[]={0,0};
    
        //for(i=0;i<3;i++)
        //cout<<a[i]<<" "<<b[i]<<" ";  
    for(i=0;i<3;i++)
{
    
    if(a[i]>b[i])
    {
        c[0]=c[0]+1;    
    }
    else if(a[i]<b[i])
    {
        c[1]=c[1]+1;
    }
}

for(i=0;i<2;i++)
cout<<c[i]<<" ";
}
